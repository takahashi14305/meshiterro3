<h1><%= @user.name %>さんのプロフィール</h1>
<table>
  <thead>
    <tr>
      <th>プロフィール画像</th>
      <th>名前</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= image_tag @user.get_profile_image(100, 100) %></td>
      <td><%= @user.name %></td>
    </tr>
  </tbody>
</table>
<%= link_to "プロフィール編集", edit_user_path(current_user) %>

<%= render 'post_images/list', post_images: @post_images %>